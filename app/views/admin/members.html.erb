<div class="col-md-6 col-md-offset-3 text-center">
      <ul class="list-group">
      <% @users.each do |user|  %>

        <li class="list-group-item"><h3> <%=link_to user.name, user%> </h3>
            <% if user.member? %>
                <div class="btn btn-warning">Clique Member</div>
                
            <% else %>
                <div class="btn btn-default">Not a Member</div>
            <% end %>

        </li>

      <%end %>

      </ul>
</div>